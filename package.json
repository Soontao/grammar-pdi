{
  "name": "grammar-pdi",
  "version": "1.0.8",
  "description": "Grammar parser for PDI BODL/ABSL",
  "main": "index.js",
  "unpkg": "./dist/grammar-pdi-umd.js",
  "scripts": {
    "build:grammar": "cd bodl && ./make_lib.sh && npm run lint",
    "build:js:umd": "webpack",
    "lint": "eslint --fix .",
    "test": "jest",
    "test:go": "go test -v -mod=vendor ./... -race -coverprofile=coverage.txt -covermode=atomic",
    "prepublishOnly": "npm run build:grammar && npm run build:js:umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soontao/grammar-pdi.git"
  },
  "author": "Theo Sun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Soontao/grammar-pdi/issues"
  },
  "homepage": "https://github.com/Soontao/grammar-pdi#readme",
  "dependencies": {
    "antlr4": "^4.7.2"
  },
  "devDependencies": {
    "@types/jest": "^24",
    "eslint": "^6.6.0",
    "jest": "^24",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
